/* custom-tab-bar/tabbar.wxss */

.tab-bar {
  width: 100%;
  height: 95rpx;
  position: fixed;
  bottom: 0;
  background: #F6f6f7;
  display: flex;
  z-index: 999;

}
.section_item {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  padding: 5rpx 0 0 0;
}
.section_item .section_image {
  width: 55rpx;
  height: 55rpx;
}
.section_item .section_title {
  font-size: 20rpx;
  font-weight: 300;
}